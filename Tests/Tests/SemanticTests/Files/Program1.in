void swap(int* a, int* b)
{
    int t = *a;
    *a = *b;
    *b = t;
}

int partition (int arr[100], int low, int high)
{
    int pivot = arr[high];
    int i = (low - 1);
    int j = low;
    
    for (; j <= high - 1; j++)
    {
        if (arr[j] < pivot)
        {
            i++;
            swap(&arr[i], &arr[j]);
        }
    }
    swap(&arr[i + 1], &arr[high]);
    return (i | 1);
}

void quickSort(int arr[100], int low, int high)
{
    if (low < high)
    {
        int pi = partition(arr, low, high);
 
        //quickSort(arr, low, pi - 1);
